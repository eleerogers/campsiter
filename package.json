{
  "name": "simple-react-full-stack",
  "version": "1.0.0",
  "description": "Campsiter is a full stack web application using React, Node.js, Express and Webpack.",
  "private": true,
  "main": "src/server/index.js",
  "scripts": {
    "build": "rm -rf ./dist && webpack -p",
    "start": "npm run build && node src/server/index.js",
    "client": "webpack-dev-server --mode development --devtool inline-source-map --hot",
    "server": "nodemon src/server/index.js",
    "dev": "export NODE_ENV=dev && concurrently \"npm run server\" \"npm run client\"",
    "webpack": "webpack -p --mode=production --profile --json > compilation-stats.json",
    "dev:analyze": "npm run dev -- --env.addons=bundleanalyzer",
    "build:analyze": "npm run build -- --env.addons=bundleanalyzer",
    "wp-dev": "webpack --mode development",
    "wp-build": "webpack --mode production",
    "test": "jest --verbose"
  },
  "author": "Lee Rogers",
  "dependencies": {
    "@babel/core": "^7.10.2",
    "@babel/plugin-proposal-class-properties": "^7.10.1",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-react-jsx": "^7.10.4",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-react": "^7.10.1",
    "@mars/heroku-js-runtime-env": "^3.0.2",
    "@react-google-maps/api": "^1.9.5",
    "axios": "^0.19.2",
    "babel-loader": "^8.1.0",
    "babel-polyfill": "^6.26.0",
    "bcrypt": "^5.0.0",
    "brotli-webpack-plugin": "^1.1.0",
    "chunks-webpack-plugin": "^6.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "cloudinary": "^1.22.0",
    "compression": "^1.7.4",
    "concurrently": "^5.2.0",
    "cookie-parser": "^1.4.5",
    "cookie-session": "^1.4.0",
    "cors": "^2.8.5",
    "css-loader": "^3.5.3",
    "dotenv": "^8.2.0",
    "dotenv-webpack": "^1.8.0",
    "express": "^4.17.1",
    "express-favicon": "^2.0.1",
    "express-session": "^1.17.1",
    "file-loader": "^6.0.0",
    "fs": "0.0.1-security",
    "html-webpack-plugin": "^4.3.0",
    "mini-css-extract-plugin": "^0.9.0",
    "moment-mini": "^2.24.0",
    "multer": "^1.4.2",
    "multer-storage-cloudinary": "^4.0.0",
    "node-geocoder": "^3.27.0",
    "nodemailer": "^6.4.16",
    "nodemon": "^2.0.4",
    "path": "^0.12.7",
    "pg": "^8.2.1",
    "preact": "^10.4.8",
    "preact-render-to-string": "^5.1.10",
    "preact-router": "^3.2.1",
    "prop-types": "^15.7.2",
    "react-bootstrap": "^1.0.1",
    "react-bootstrap-icons": "^1.0.3-alpha5",
    "react-rating": "^2.0.5",
    "react-router": "5.2.0",
    "react-router-dom": "5.2.0",
    "react-spring": "^8.0.27",
    "react-test-renderer": "^16.13.1",
    "react-toastify": "^6.0.5",
    "serve-favicon": "^2.5.0",
    "style-loader": "^1.2.1",
    "url-loader": "^4.1.0",
    "util": "^0.12.3",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0"
  },
  "devDependencies": {
    "babel-jest": "^26.3.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-preact-pure": "^2.2.3",
    "enzyme-to-json": "^3.5.0",
    "eslint": "^7.2.0",
    "eslint-plugin-jest": "^23.20.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^4.0.4",
    "eslint-plugin-react-perf": "^3.2.4",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.0.1-alpha.0",
    "jest-preset-preact": "^4.0.2",
    "preact-transition-group": "^2.0.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "webpack-bundle-analyzer": "^3.8.0"
  },
  "engines": {
    "node": "13.x",
    "npm": "6.x"
  },
  "plugins": [
    "react-perf"
  ],
  "setupFiles": [
    "./src/setupTests.js"
  ],
  "jest": {
    "moduleNameMapper": {
      "^react$": "preact/compat",
      "^react-dom$": "preact/compat",
      "^react-dom/test-utils$": "preact/test-utils"
    }
  }
}
